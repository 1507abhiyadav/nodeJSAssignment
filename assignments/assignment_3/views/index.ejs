<!DOCTYPE html>
<html>
<head>
    <title></title>

<style>
    table, th, td {
      border:1px solid black;
    }
    </style>

</head>
    <body>
    
    <h2>A basic HTML table</h2>
    
    <table style="width:100%">
      <tr>
        <th>Name</th>
        <th>Age</th>
        <th>Email</th>
        <th>City</th>
        <th>Profession</th>
      </tr>
      <% users.forEach(user => { %>
        <tr>
            <td><%= user.name%></td>
            <td><%= user.age%></td>
            <td><%= user.email%></td>
            <td><%= user.city%></td>
            <td><%= user.profession%></td>
          </tr>
     <% })  %>
      
    </table>
    <a href="/form">Add user</a>
    </body>
    </html>